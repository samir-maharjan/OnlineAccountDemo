@model IEnumerable<OnlineAccountDemo.Models.BrandModel>

@{
    ViewData["Title"] = "Model List";
    Layout = "~/Views/Shared/_ISMLayout.cshtml";
}




<div class="container mt-5">
    <a href="@Url.Action("CreateBrandModel","BrandModel")" class="btn btn-outline-success">Add New Model</a>
    <a href="@Url.Action("ListBrand","RepairCategory")" class="btn btn-outline-success">DashBoard</a>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Model Title</th>
                <th scope="col">Model Code</th>
                <th scope="col">Brand</th>
                <th scope="col">Status</th>
                <th scope="col">Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ModelTitle</td>
                    <td>@item.ModelCode</td>
                    <td>@item.BrandCategory.BrandTitle</td>
                    <td>@item.Status</td>
                    <td></td>
                </tr>
            }

        </tbody>
    </table>
</div>


@section Scripts {

}




